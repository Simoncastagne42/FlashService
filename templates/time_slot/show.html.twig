{% extends 'base.html.twig' %}

{% block title %}TimeSlot{% endblock %}

{% block body %}
    <h1>TimeSlot</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ time_slot.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ time_slot.date ? time_slot.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>HeureDebut</th>
                <td>{{ time_slot.heureDebut ? time_slot.heureDebut|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>HeureFin</th>
                <td>{{ time_slot.heureFin ? time_slot.heureFin|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('time_slot_index') }}">back to list</a>

    <a href="{{ path('time_slot_edit', {'id': time_slot.id}) }}">edit</a>

    {{ include('time_slot/_delete_form.html.twig') }}
{% endblock %}
